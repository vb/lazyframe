{
  "name": "lazyframe",
  "version": "1.1.7",
  "description": "Dependency-free library for lazyloading iframes",
  "main": "dist/lazyframe.min.js",
  "jsnext:main": "src/lazyframe.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vb/lazyframe.git"
  },
  "keywords": [
    "lazyload",
    "lazyloading",
    "embed",
    "iframe",
    "youtube",
    "vimeo",
    "vine",
    "pagespeed",
    "pageload",
    "javascript"
  ],
  "author": "Viktor Bergehall",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vb/lazyframe/issues"
  },
  "homepage": "https://vb.github.io/lazyframe/",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "node-sass": "^4.11.0",
    "rollup": "^1.0.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-uglify": "^6.0.0"
  },
  "scripts": {
    "build": "npm run sass && npm run js",
    "js": "rollup --config ./rollup.config.js",
    "sass": "node-sass --output-style compressed src/scss/lazyframe.scss dist/lazyframe.css",
    "test": "npm run build"
  }
}
